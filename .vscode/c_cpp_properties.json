{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/external/fmt/include/fmt/**",
                "${workspaceFolder}/external/ftxui/include/ftxui/**"
            ],
            "defines": [],
            "compilerPath": "/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-L@/usr/lib",
                "-lstdc++",
                "-lm",
                "-Llib",
                "-lfmt",
                "-lftxui-component",
                "-lftxui-screen",
                "-lftxui-dom"
            ]
        }
    ],
    "version": 4
}